<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
<bean id="setinject" class="ustbatchno3.DI_setterinjection.Roles">
<property name="charactername" value="Anu"></property>	
<property name="responsibility" value="Daughter"></property>	

</bean>
<bean id="setter" class="ustbatchno3.DI_setterinjection.Roles">
